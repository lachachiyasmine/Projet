        <script>
          $(function () {
            const SESSION_NAME = '_monmedenligne_popup_window';
            var $modal = $('#popupModal');
            if ($modal.length > 0) {
              var value = sessionStorage.getItem(SESSION_NAME);
              if (!value) {
                $modal.modal();
              }

              $modal.on('hidden.bs.modal', function () {
                sessionStorage.setItem(SESSION_NAME, 1);
              });
            }

            $('.dropdown-country-picker').on('click', function () {
              $.ajax({
                type: 'POST',
                url: '/change-selected-country',
                data: {selectedCountry: $(this).data('value')},
                success: function (data) {
                  window.location.reload();
                },
                error: function (data) {

                }
              })
            });
          });
        </script>
    
    <script src="assets/js/autocomplete/jquery.autocomplete.min.js"></script>

    <script>
      $('.js-autocomplete').autocomplete({
        lookup: function (query, done) {
          if (query && query.length >= 2) {
            $.ajax({
              type: 'POST',
              url: '/search-autocomplete',
              data: {search: query},
              success: function (data) {
                var result = {
                  suggestions: data
                };

                done(result);
              }
            });
          }
        },
        onSelect: function (suggestion) {
          if (suggestion.doctor && suggestion.slug) {
            window.location.href = '/doctor/profile/' + suggestion.slug;
          } else if (suggestion.clinic && suggestion.slug) {
            window.location.href = '/clinic/profile/' + suggestion.slug;
          } else if (suggestion.hospital && suggestion.slug) {
            window.location.href = '/hospital/profile/' + suggestion.slug;
          } else if (suggestion.medical && suggestion.slug) {
            window.location.href = '/medical/profile/' + suggestion.slug;
          }
        },
        formatResult: function (suggestion) {
          var image = '/assets/images/login/doctor.png';

          switch (suggestion.type) {
            case 'DOCTOR':
              image = suggestion.image ? '/uploads/doctor/' + suggestion.image : '/assets/images/login/doctor.png';
              break;
            case 'CLINIC':
              image = suggestion.image ? '/uploads/clinic/' + suggestion.image : '/assets/images/login/doctor.png';
              break;
            case 'HOSPITAL':
              image = suggestion.image ? '/uploads/hospital/' + suggestion.image : '/assets/images/login/doctor.png';
              break;
            case 'MEDICAL':
              image = suggestion.image ? '/uploads/medical/' + suggestion.image : '/assets/images/login/doctor.png';
              break;
          }

          return '<div class="result-item" style=""><div class="result-item--img"><img src="https://www.monmedenligne.com/'&#32;+&#32;image&#32;+'" style="max-width: 50px;" /></div> <div class="result-item--info">' + suggestion.value + '</div></div>';
        }
      });
    </script>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAFHnd93ZXuVLX-rRep6N-Q8PaadpB7ONY&amp;&amp;region=BJ&amp;libraries=places"></script>
    <script type="text/javascript">
      //<![CDATA[

      var map; // Global declaration of the map
      var lat_longs_map = new Array();
      var markers_map = new Array();
      var iw_map;
      var placesService;

      iw_map = new google.maps.InfoWindow();

      function initialize_map() {
        var myLatlng = new google.maps.LatLng(6.373887, 2.353049);
        var myOptions = {
          zoom: 13,
          center: myLatlng,
          mapTypeId: google.maps.MapTypeId.ROADMAP
        }
        map = new google.maps.Map(document.getElementById('google_map_canvas'), myOptions);

        addMapsMarker(map);
      }

      function addMapsMarker (map) {
        $('.js-doctor-map-data').each(function () {
          var $this = $(this);
          var myLatlng = new google.maps.LatLng($this.data('latitude'), $this.data('longitude'));

          var markerOptions = {
            map: map,
            position: myLatlng,
          };
          var marker = createMarker_map(markerOptions);
          marker.set('content', '<a href="https://www.monmedenligne.com/\&quot;https://www.google.com/maps/search/?api=1&amp;query='+$this.data('latitude')+','+$this.data('latitude')+'" target="new"><h2>' + $this.data('name') + '</h2><h5>' + $this.data('address') + '</h5></a>');

          google.maps.event.addListener(marker, 'click', function (event) {
            iw_map.setContent(this.get('content'));
            iw_map.open(map, this);
          });
        })
      }

      function createMarker_map(markerOptions) {
        var marker = new google.maps.Marker(markerOptions);
        markers_map.push(marker);
        lat_longs_map.push(marker.getPosition());
        return marker;
      }

      google.maps.event.addDomListener(window, "load", initialize_map);

      var navigateCalendar = function ($wrapper) {
        $wrapper.on('click', '.next-cal-app', function () {
          var $this = $(this);
          var currentDate = $this.attr('data-date');
          var columns = $this.attr('data-columns');
          // var $element = $wrapper.find('.js-calendar-app');

          loadCalendar($this.attr('id'), 'next', $wrapper, currentDate, columns);
        });

        /* === Prev === */
        $wrapper.on('click', '.previous-cal-app', function () {
          var $this = $(this);
          var currentDate = $this.attr('data-date');
          var columns = $this.attr('data-columns');
          // var $element = $wrapper.find('.js-calendar-app');

          loadCalendar($this.attr('id'), 'prev', $wrapper, currentDate, columns);
        });

        function loadCalendar(id, status, $element, dateCnt, columns) {
          var bookingType = 'home';
          var options = bookingType === 'center' ? {default: true} : {};

          $.ajax({
            type: 'POST',
            url: '/doctor/doctor-main-calendar/'+ id,
            data: {dateCalendar: dateCnt, status, bookingType, options, columns},
            success: function (result) {
              $element.html(result);
              bookAppointmentTime($element.find('.js-calendar-time'));
            }
          });
        }
      }

      function bookAppointmentTime($calendarTime) {
        var calendarDate = $calendarTime.data('calendar-date');
        var calendarTime = $calendarTime.data('calendar-time');
        var calendarDateTime = new Date(calendarDate + ' ' + calendarTime);
        var currentDateTime = new Date();

        if (currentDateTime >= calendarDateTime) {
          $calendarTime.addClass('hide');
        }

        $calendarTime.on('click', function gettimecalendar() {
          var $element = $(this),
            $wrapper = $element.closest('.js-calendar-wrapper'),
            doctorId = $wrapper.data('doctor');

          if (currentDateTime >= calendarDateTime) {
            alert("Désolé.Cette heure est déjà expirée.Veuillez essayer une autre");
          } else {

            var consultationType = 'home';
            var query = '?consultationType=' + consultationType;
            if (consultationType === 'center') {
              var centerId = $wrapper.data('center-id');
              var centerType = $wrapper.data('center-type');

              query += '&centerId=' + centerId + '&centerType=' + centerType;
            }
            if (doctorId && calendarDate && calendarTime && ['Booked','In vacation','Break'].indexOf(calendarTime) === -1) {
              window.location.href = '/doctor/booking' + '/' + doctorId + '/' + calendarDate + '/' + calendarTime + query;
            }
          }
        });
      }

      $(document).ready(function () {
        document.getElementById('result-wrapper').scrollIntoView({ behavior: 'smooth' });

        $('.select2').select2();

        $('.js-calendar-wrapper').each(function () {
          navigateCalendar($(this));
        });

        $('.js-calendar-time').each(function () {
          bookAppointmentTime($(this));
        })

        $('.gc-ratting').each(function() {
          var rate = $(this).data('rate');
          $(this).raty({
            readOnly: true,
            score: rate, //default stars
          });
        });

        $('form.form-doctor .row-secondary select').on('change', function (e) {
          $(this).closest('form').submit();
        });

        $('form.form-doctor .js-select-speciality').on('change', function (e) {
          var $this = $(this);
          var value = $this.val();

          var $select = $this.closest('form').find('select.js-visitation');
          if ($select.length > 0) {
            $select.find('option').each(function () {
              var $option = $(this);
              if (!value) {
                $option.show();
              }
              else if ($option.data('speciality') == value) {
                $option.show();
              }
              else {
                $option.hide();
              }
            })
          }
        });

        $('form.form-doctor').on('submit', function (e) {
          e.preventDefault();

          var $form = $(this);
          var arrayData = $form.serializeArray();
          var params = {}

          arrayData.forEach(function (item) {
            if (item.name) {
              var key = item.name.replace(/search|\[|\]/g,'');
              if (typeof key !== 'undefined' && key !== '_token' && item.value) {
                params[key] = item.value;
              }
            }
          });

          var queryString = Object.keys(params).map(function(key) {
            return key + '=' + params[key]
          }).join('&');

          window.location.href = '/search?' + queryString;
        })

        $(window).scroll(function() {
          var scroll = $(window).scrollTop();
          if (scroll >= 500) {
            $('.google-map-doctor').addClass('sticky-map');
          }
          else {
            $('.google-map-doctor').removeClass('sticky-map');
          }
        });

        $(window).scroll(function() {
          var scroll = $(window).scrollTop();
          if (scroll >= 300) {
            $('.search-form .row-secondary').addClass('sticky');
          }
          else {
            $('.search-form .row-secondary').removeClass('sticky');
          }
        });

        $(window).scroll(function() {
          var scroll = $(window).scrollTop();
          if (scroll >= 600) {
            $('.search-tabs').addClass('sticky');
          }
          else {
            $('.search-tabs').removeClass('sticky');
          }
        });

        $('.booking-button').on('click', function () {
          var $this = $(this);

          $('.booking-button').removeClass('active');
          $this.addClass('active');

          var $form = $this.closest('form');
          $form.find('#bookingType').val($this.data('type'));
          // $form.submit();
        });

        $('.search-tabs .res-tab').on('click', function () {
          var $element = $('.results-container');
          $element.removeClass('map-mode');
          $element.addClass('results-mode');
          $('form .search-form').removeClass('filter-mode');
        });

        $('.search-tabs .map-tab').on('click', function () {
          var $element = $('.results-container');
          $element.addClass('map-mode');
          $element.removeClass('results-mode');
          $('form .search-form').removeClass('filter-mode');
        });

        $('.search-tabs .filter-tab').on('click', function () {
          var $element = $('.results-container');
          $element.removeClass('map-mode');
          $element.removeClass('results-mode');

          var $form = $('form .search-form');
          $form.addClass('filter-mode');
          $('html, body').animate({
            scrollTop: $form.find('.row-secondary').offset().top - 100
          }, 200);
        });
      });
      //]]>
    </script>
